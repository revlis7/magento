<?xml version="1.0"?>
<config>
    <modules>
        <Tnc_Customer>
            <version>0.0.1</version>
        </Tnc_Customer>
    </modules>
    <global>
        <helpers>
            <customer>
                <rewrite>
                    <data>Tnc_Customer_Helper_Data</data>
                </rewrite>
            </customer>
        </helpers>
    </global>
    <frontend>
        <routers>
            <customer>
                <use>standard</use>
                <args>
                    <module>Tnc_Customer</module>
                    <frontName>customer</frontName>
                </args>
            </customer>
        </routers>
        <layout>
            <updates>
                <customer module="Tnc_Customer">
                    <file>tnc_customer.xml</file>
                </customer>
            </updates>
        </layout>
    </frontend>
</config>
